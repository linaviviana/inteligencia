  		var i;
  		document.write('lista <br>');
    			for(l=0;l<numeros.length;l++)
    			{
      			var v;
      				v=prompt('Ingrese numero:','');
      				numeros[l]=parseInt(v); 
      						if(numeros[l]%2==0) {
      						        
      		document.write('par ',numeros[l],'<br>');    	
      													}
      			else {
      	document.write('impar',numeros[l],'<br>');
      }
    }
    
 					
   } 
  
  
  	
   
  
  var numeros;
